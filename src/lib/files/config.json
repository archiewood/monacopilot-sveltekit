                "suggestionType": "column"
            },
            "dimensions": {
                "required": false,
                "suggestionType": "column"
            },
            "measures": {
                "required": false,
                "suggestionType": "column"
            },
            "width": {
                "required": false,
                "default": 12,
                "description": "Width (in columns) of the table on the page grid"
            },
            "height": {
                "required": false,
                "default": 3,
                "description": "Height (in rows) of the table on the page grid"
            }
        }
    },
    "value": {
        "render": "value",
        "selfClosing": true,
        "isInline": true,
        "description": "Display a single value from a database query",
        "attributes": {
            "data": {
                "required": true,
                "suggestionType": "table",
                "description": "Table or view to query"
            },
            "value": {
