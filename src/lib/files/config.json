{
    "area_chart": {
        "render": "area_chart",
        "selfClosing": true,
        "description": "Display a area chart",
        "grid": {
            "userCanChangeWidth": true,
            "userCanChangeHeight": true,
            "defaultWidth": 12,
            "defaultHeight": 3,
            "minWidth": 300
        },
        "attributes": {
            "data": {
                "required": true,
                "description": "Name of the table to query",
                "suggestionType": "table"
            },
            "filters": {
                "required": false,
                "default": [],
                "description": "Array of column names to use as filters",
                "suggestionType": "filter"
            },
            "x": {
                "required": true,
                "description": "Column name for x-axis",
                "suggestionType": "column"
            },
            "y": {
                "required": true,
                "description": "Column name for y-axis",
                "suggestionType": "column"
            },
            "series": {
                "required": false,
                "description": "Column name for series",
                "suggestionType": "column"
            },
            "size": {
                "required": false,
                "description": "Column name for size",
                "suggestionType": "column"
            },
            "title": {
                "required": false,
                "description": "Title to display above the chart"
            },
            "subtitle": {
                "required": false,
                "description": "Subtitle to display below the title"
            },
            "x_label_wrap": {
                "required": false,
                "default": false,
                "description": "Wrap x-axis labels"
            },
            "time_grain": {
                "required": false,
                "matches": [
                    "day",
                    "week",
                    "month",
                    "quarter",
                    "year"
                ],
                "default": "day",
                "description": "Time grain for date truncation (default: day for date columns)"
            },
            "agg": {
                "required": false,
                "matches": [
                    "sum",
                    "avg",
                    "mean",
                    "max",
                    "min",
                    "count",
                    "count_distinct"
                ],
                "description": "Aggregation function for y-axis (default: sum for numeric columns, count for date and string columns)"
            },
            "y_fmt": {
                "required": false,
                "default": "num",
                "description": "Format for y-axis values"
            },
            "x_fmt": {
                "required": false,
                "default": "num",
                "description": "Format for x-axis values"
            },
            "echarts_options": {
                "required": false,
                "description": "ECharts configuration options",
                "default": {}
            },
            "renderer": {
                "required": false,
                "default": "canvas",
                "matches": [
                    "canvas",
                    "svg"
                ],
                "description": "Renderer to use for the chart"
            },
            "order": {
                "required": false,
                "description": "Column name(s) with optional direction (e.g. \"column_name\", \"column_name desc\")",
                "suggestionType": "column"
            },
            "stretch": {
                "required": false,
                "default": false,
                "description": "Stretches the chart area to fill all available space when a chart is placed next to a taller chart"
            },
            "width": {
                "required": false,
                "default": 12,
                "description": "Width (in columns) of the area_chart on the page grid"
            },
            "height": {
                "required": false,
                "default": 3,
                "description": "Height (in rows) of the area_chart on the page grid"
            }
        }
    },
