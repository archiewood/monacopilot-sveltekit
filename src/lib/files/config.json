    "delta": {
        "render": "delta",
        "selfClosing": true,
        "isInline": true,
        "description": "Display a delta value with an up/down indicator",
        "attributes": {
            "data": {
                "required": true,
                "suggestionType": "table",
                "description": "Table or view to query"
            },
            "value": {
                "required": true,
                "description": "SQL expression to insert into the SELECT part of the query (e.g., \"COUNT(*)\", \"SUM(sales)\")",
                "suggestionType": "column"
            },
            "className": {
                "required": false,
                "description": "CSS class to apply to the delta"
            },
            "fmt": {
                "required": false,
                "description": "Format code for the value (e.g., \"num\", \"usd\", \"pct\"). See formatValue documentation for available formats."
            },
            "text": {
                "required": false,
                "description": "Text appearing after the delta (e.g., vs. prev month)"
            },
            "chip": {
                "required": false,
                "default": false,
                "description": "Whether to display as a chip"
            },
            "downIsGood": {
                "required": false,
                "default": false,
                "description": "Whether a downward trend is considered positive"
            },
            "showValue": {
                "required": false,
                "default": true,
                "description": "Whether to show the value"
            },
            "showSymbol": {
                "required": false,
                "default": true,
                "description": "Whether to show the delta symbol"
            },
            "symbolPosition": {
                "required": false,
                "default": "right",
                "matches": [
                    "left",
                    "right"
                ],
                "description": "Position of the delta symbol relative to the value"
            },
            "align": {
                "required": false,
                "default": "right",
                "matches": [
                    "left",
                    "center",
                    "right"
                ],
                "description": "Text alignment"
            },
            "neutralMin": {
                "required": false,
                "default": 0,
                "description": "Minimum value to be considered neutral"
            },
            "neutralMax": {
                "required": false,
                "default": 0,
                "description": "Maximum value to be considered neutral"
            },
            "filters": {
                "required": false,
                "description": "IDs of filters to apply to the query",
                "suggestionType": "filter"
            }
        }
    },
    "dropdown": {
        "render": "dropdown",
        "description": "Display a dropdown with distinct values from a database column",
        "grid": {
            "userCanChangeWidth": true,
            "userCanChangeHeight": false,
            "defaultWidth": 6,
            "defaultHeight": 1
        },
        "attributes": {
            "id": {
                "description": "The id of the dropdown to be used in a `filters` prop",
                "required": true
            },
            "data": {
                "description": "Name of the table to query",
                "required": false,
                "suggested": true,
                "suggestionType": "table"
            },
            "value_column": {
                "description": "Column name to use as the value for each option, and the column to filter by when this dropdown's `id` is used in the `filters` prop of a chart",
                "required": false,
                "suggested": true,
                "suggestionType": "column"
            },
            "label_column": {
                "description": "Column name to use as the label for each option",
                "required": false
            },
            "options": {
                "description": "List of options to display in the dropdown",
                "required": false
            },
            "title": {
                "description": "Text displayed above the dropdown",
                "required": false
            },
            "initial_value": {
                "type": [
                    null,
                    null
                ],
                "description": "Initial selected value(s)",
                "required": false
            },
            "placeholder": {
                "description": "Placeholder text displayed when no value is selected",
                "required": false
            },
            "search": {
                "description": "Includes a search input within the dropdown menu",
                "required": false,
                "default": false
            },
            "multiple": {
                "description": "Allows multiple selections",
                "required": false,
                "default": false
            },
            "clear": {
                "description": "Includes a clear button to unselect the selected value(s)",
                "required": false,
                "default": true
            },
            "order": {
                "description": "Column name(s) with optional direction (e.g. \"column_name\", \"column_name desc\")",
                "required": false,
                "suggestionType": "column"
            },
            "width": {
                "required": false,
                "default": 6,
                "description": "Width (in columns) of the dropdown on the page grid"
            }
        }
    },
