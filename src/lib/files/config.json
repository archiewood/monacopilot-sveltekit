        "description": "Repeats the children of this component once for each distinct value of a column",
        "selfClosing": false,
        "noWrapper": true,
        "grid": {
            "userCanChangeWidth": false,
            "userCanChangeHeight": false
        },
        "attributes": {
            "id": {
                "required": true,
                "description": "The id of this repeat component to be used in the `filters` prop of its children"
            },
            "data": {
                "required": true,
                "description": "The name of the table to query",
                "suggestionType": "table"
            },
            "column": {
                "required": true,
                "description": "The name of the column within the `data` table to get distinct values",
                "suggestionType": "column"
            }
        }
    },
    "scatter_chart": {
        "render": "scatter_chart",
        "selfClosing": true,
        "description": "Display a scatter chart",
        "grid": {
            "userCanChangeWidth": true,
            "userCanChangeHeight": true,
            "defaultWidth": 12,
            "defaultHeight": 3,
            "minWidth": 300
        },
        "attributes": {
            "data": {
                "required": true,
                "description": "Name of the table to query",
                "suggestionType": "table"
            },
            "filters": {
                "required": false,
                "default": [],
                "description": "Array of column names to use as filters",
                "suggestionType": "filter"
            },
            "x": {
                "required": true,
                "description": "Column name for x-axis",
                "suggestionType": "column"
            },
            "y": {
                "required": true,
                "description": "Column name for y-axis",
                "suggestionType": "column"
            },
            "series": {
                "required": false,
                "description": "Column name for series",
                "suggestionType": "column"
            },
            "size": {
                "required": false,
                "description": "Column name for size",
                "suggestionType": "column"
            },
            "title": {
                "required": false,
                "description": "Title to display above the chart"
            },
            "subtitle": {
                "required": false,
                "description": "Subtitle to display below the title"
            },
            "x_label_wrap": {
                "required": false,
                "default": false,
                "description": "Wrap x-axis labels"
            },
            "time_grain": {
                "required": false,
                "matches": [
                    "day",
                    "week",
                    "month",
                    "quarter",
                    "year"
                ],
                "default": "day",
                "description": "Time grain for date truncation (default: day for date columns)"
            },
            "agg": {
                "required": false,
                "matches": [
                    "sum",
                    "avg",
                    "mean",
                    "max",
                    "min",
                    "count",
                    "count_distinct"
                ],
                "description": "Aggregation function for y-axis (default: sum for numeric columns, count for date and string columns)"
            },
            "y_fmt": {
                "required": false,
                "default": "num",
                "description": "Format for y-axis values"
            },
            "x_fmt": {
                "required": false,
                "default": "num",
                "description": "Format for x-axis values"
            },
            "echarts_options": {
                "required": false,
                "description": "ECharts configuration options",
                "default": {}
            },
            "renderer": {
                "required": false,
                "default": "canvas",
                "matches": [
                    "canvas",
                    "svg"
                ],
                "description": "Renderer to use for the chart"
            },
            "order": {
                "required": false,
                "description": "Column name(s) with optional direction (e.g. \"column_name\", \"column_name desc\")",
                "suggestionType": "column"
            },
            "stretch": {
                "required": false,
                "default": false,
                "description": "Stretches the chart area to fill all available space when a chart is placed next to a taller chart"
            },
            "opacity": {
                "required": false,
                "default": 0.7,
                "description": "The opacity of the series"
            },
            "width": {
                "required": false,
                "default": 12,
                "description": "Width (in columns) of the scatter_chart on the page grid"
            },
            "height": {
                "required": false,
                "default": 3,
                "description": "Height (in rows) of the scatter_chart on the page grid"
            }
        }
    },
    "sparkline": {
        "render": "sparkline",
        "selfClosing": true,
        "isInline": true,
        "description": "Display a small inline sparkline chart",
        "attributes": {
            "data": {
                "required": true,
                "suggestionType": "table",
                "description": "Table or view to query"
            },
            "x": {
                "required": true,
                "description": "Column for x-axis (often date/time)",
                "suggestionType": "column"
            },
            "y": {
                "required": true,
                "description": "Column for y-axis (values to plot)",
                "suggestionType": "column"
            },
            "agg": {
                "required": false,
                "description": "Aggregation to use (e.g., \"sum\", \"avg\", \"min\", \"max\", \"count\", \"count_distinct\")",
                "matches": [
                    "sum",
                    "avg",
                    "min",
                    "max",
                    "count",
                    "count_distinct"
                ]
            },
            "time_grain": {
                "required": false,
                "description": "Time grain for date columns (e.g., \"day\", \"week\", \"month\", \"quarter\", \"year\")",
                "matches": [
                    "day",
                    "week",
                    "month",
                    "quarter",
                    "year"
                ]
            },
            "type": {
                "required": false,
                "default": "line",
                "matches": [
                    "line",
                    "area",
                    "bar"
                ],
                "description": "Type of sparkline to display"
            },
            "color": {
                "required": false,
                "description": "Color of the sparkline (CSS color)"
            },
            "y_fmt": {
                "required": false,
                "description": "Format for y-axis tooltip (e.g., \"num\", \"usd\", \"pct\")"
            },
            "x_fmt": {
                "required": false,
                "description": "Format for x-axis tooltip (e.g., \"shortdate\", \"longdate\")"
            },
            "y_scale": {
                "required": false,
                "default": false,
                "description": "Whether to scale the y-axis to the data range"
            },
            "interactive": {
                "required": false,
                "default": true,
                "description": "Whether the sparkline should be interactive"
            },
            "class_name": {
                "required": false,
                "description": "Additional CSS classes to apply"
            },
            "filters": {
                "required": false,
                "description": "IDs of filters to apply to the query",
                "suggestionType": "filter"
            },
            "order": {
                "required": false,
                "description": "Column name(s) with optional direction (e.g. \"column_name\", \"column_name desc\")",
                "suggestionType": "column"
            }
        }
    },
    "stack": {
        "render": "stack",
        "description": "Stacks its children vertically",
        "attributes": {},
